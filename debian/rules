include /usr/share/cdbs/1/class/gnome.mk

clean::
	rm -f intltool-{update,merge,extract} po/.intltool-merge-cache
	find help -name '*omf.out' -exec rm -f \{\} \;
	find help -name 'omf_timestamp' -exec rm -f \{\} \;
