<Root>

<commands>

	<cmd name="Previous"
	     pixtype="stock"
	     pixname="rhythmbox-previous"
	     _tip="Start playing the previous song"
	     priority="1"/>
	     
	<cmd name="Play"
	     pixtype="stock"
	     pixname="rhythmbox-play"
	     _tip="Start playing"
	     priority="1"/>

	<cmd name="Pause"
	     pixtype="stock"
	     pixname="rhythmbox-pause"
	     _tip="Pause playing"
	     priority="1"/>

	<cmd name="Stop"
	     pixtype="stock"
	     pixname="rhythmbox-stop"
	     _tip="Stop playing"
	     priority="1"/>

	<cmd name="Next"
	     pixtype="stock"
	     pixname="rhythmbox-next"
	     _tip="Start playing the next song"
	     priority="1"/>

	<cmd name="Shuffle"
	     pixtype="stock"
	     pixname="rhythmbox-shuffle"
	     _tip="Toggle random playing order"
	     priority="1"
	     type="toggle"/>

	<cmd name="Repeat"
	     pixtype="stock"
	     pixname="rhythmbox-repeat"
	     _tip="Toggle repeat mode"
	     priority="1"
	     type="toggle"/>

	<cmd name="RestartSong"
	     pixtype="stock"
	     pixname="rhythmbox-rewind"
	     _tip="Restart song from the beginning"/>

	<cmd name="Cut"
	     pixtype="stock"
	     pixname="gtk-cut"
	     _tip="Cut selection"/>

	<cmd name="Copy"
	     pixtype="stock"
	     pixname="gtk-copy"
	     _tip="Copy selection"/>

	<cmd name="Paste"
	     pixtype="stock"
	     pixname="gtk-paste"
	     _tip="Paste selection"/>

	<cmd name="Delete"
	     _tip="Delete selection"/>

	<cmd name="About"
	     pixtype="stock"
	     pixname="gnome-stock-about"
	     _tip="Show information about Rhythmbox"/>

	<cmd name="Contents"
	     pixtype="stock"
	     pixname="gtk-help"
	     accel="F1" 
	     _tip="Display Rhythmbox Help"/>

	<cmd name="Quit"
	     pixtype="stock"
	     pixname="gtk-quit"
	     _tip="Quit music player"/>

	<cmd name="Preferences"
	     pixtype="stock"
	     pixname="gtk-preferences"
	     _tip="Edit music player preferences"/>

	<cmd name="NewGroup"
	     pixtype="stock"
	     pixname="gtk-new"
	     _tip="Create a new music group"/>

	<cmd name="AddToLibrary"
	     pixtype="stock"
	     pixname="gtk-open"
	     _tip="Choose files or a directory to be added to the library"/>

	<cmd name="ShowToolbar"
	     _tip="Change the visibility of the toolbar"
	     type="toggle"/>

	<cmd name="ShowStatusbar"
	     _tip="Change the visibility of the statusbar"
	     type="toggle"/>

	<cmd name="ShowSidebar"
	     _tip="Change the visibility of the sidebar"
	     type="toggle"/>
	     
	<cmd name="ShowWindow"
	     _tip="Change the visibility of the main window"
	     type="toggle"/>
	
	<cmd name="EditToolbar"
	     _tip="Edit the toolbar"/>
	     
</commands>

<menu>

<submenu name="File" _label="_Organize">
        
        <menuitem name="NewGroup" verb="NewGroup" _label="_New Group..." accel="*Control*n"/>

	<placeholder name="SourceMenu"/>

	<separator/>

        <menuitem name="AddToLibrary" verb="AddToLibrary" _label="_Add to Library..." accel="*Control*o"/>

	<placeholder name="FileMenu"/>

	<separator/>

        <menuitem name="Quit" verb="Quit" _label="_Quit" accel="*Control*q"/>

</submenu>

<submenu name="Edit" _label="_Edit">

        <menuitem name="Cut" verb="Cut" _label="Cu_t" accel="*Control*x"/>
	
        <menuitem name="Copy" verb="Copy" _label="_Copy" accel="*Control*c"/>
	
        <menuitem name="Paste" verb="Paste" _label="_Paste" accel="*Control*v"/>

	<menuitem name="Delete" verb="Delete" _label="_Delete" accel="Delete"/>

	<placeholder name="EditMenu"/>

	<separator/>

	<menuitem name="EditToolbar" verb="EditToolbar" _label="T_oolbar"/>

	<menuitem name="Preferences" verb="Preferences" _label="Prefere_nces"/>

</submenu>

<submenu name="View" _label="_View">

	<menuitem name="Toolbar" verb="ShowToolbar" _label="_Toolbar"/>
	
	<menuitem name="Sidebar" verb="ShowSidebar" _label="S_idebar"/>

	<menuitem name="Statusbar" verb="ShowStatusbar" _label="_Statusbar"/>

	<placeholder name="ViewMenu"/>

</submenu>

<submenu name="Controls" _label="_Music">

	<menuitem name="Play" _label="_Play" verb="Play" accel="*Control*space"/>

	<separator/>

	<menuitem name="Previous" _label="P_revious" verb="Previous" accel="*Control*Left"/>

	<menuitem name="Next" _label="_Next" verb="Next" accel="*Control*Right"/>

	<menuitem name="RestartSong" _label="R_estart" verb="RestartSong" accel="*Control*r"/>

	<separator/>
	
	<menuitem name="Shuffle" _label="_Shuffle" verb="Shuffle" accel="*Control*s"/>
	
	<menuitem name="Repeat" _label="Repea_t" verb="Repeat" accel="*Control*t"/>

</submenu>

<submenu name="Help" _label="_Help">

	<menuitem name="Contents" verb="Contents" _label="_Contents"/>

        <menuitem name="About" verb="About" _label="_About"/>

</submenu>

</menu>

<popups>

	<popup name="TrayPopup">

		<menuitem name="Play" _label="_Play" verb="Play"/>
		
		<separator/>

		<menuitem name="Previous" _label="P_revious" verb="Previous"/>

		<menuitem name="Next" _label="_Next" verb="Next"/>

		<menuitem name="RestartSong" _label="R_estart" verb="RestartSong"/>

		<separator/>

		<menuitem name="ShowWindow" _label="_Show Window" verb="ShowWindow"/>
	
	</popup>

</popups>

<dockitem name="Toolbar" behavior="exclusive,never vertical" config="0">
</dockitem>

</Root>
