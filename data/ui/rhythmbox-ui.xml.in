<Root>

<commands>

	<cmd name="Previous"
	     pixtype="stock"
	     pixname="rhythmbox-previous"
	     _tip="Go to the previous track"
	     priority="1"/>
	     
	<cmd name="Play"
	     pixtype="stock"
	     pixname="rhythmbox-play"
	     _tip="Start playing"
	     priority="1"/>

	<cmd name="Pause"
	     pixtype="stock"
	     pixname="rhythmbox-pause"
	     _tip="Pause playing"
	     priority="1"/>

	<cmd name="Stop"
	     pixtype="stock"
	     pixname="rhythmbox-stop"
	     _tip="Stop playing"
	     priority="1"/>

	<cmd name="Next"
	     pixtype="stock"
	     pixname="rhythmbox-next"
	     _tip="Go to the next track"
	     priority="1"/>

	<cmd name="Shuffle"
	     pixtype="stock"
	     pixname="rhythmbox-shuffle"
	     _tip="Random playing order"
	     priority="1"
	     type="toggle"/>

	<cmd name="Repeat"
	     pixtype="stock"
	     pixname="rhythmbox-repeat"
	     _tip="Toggle repeat mode"
	     priority="1"
	     type="toggle"/>

	<cmd name="Cut"
	     pixtype="stock"
	     pixname="gtk-cut"
	     _tip="Cut selection"/>

	<cmd name="Copy"
	     pixtype="stock"
	     pixname="gtk-copy"
	     _tip="Copy selection"/>

	<cmd name="Paste"
	     pixtype="stock"
	     pixname="gtk-paste"
	     _tip="Paste selection"/>
        
</commands>

<menu>

<submenu name="File" _label="_File">
        
        <menuitem name="FileNewWindow" verb="" _label="_New Group"
	pixtype="stock" pixname="gtk-new"/>
        <menuitem name="FileNewWindo" verb="" _label="_Add to Library..."
	pixtype="stock" pixname="gtk-open"/>

	<separator/>

        <menuitem name="FileNewWind" verb="" _label="Get _Info..."
	pixtype="stock" pixname="gtk-properties"/>
	
	<separator/>
	
        <menuitem name="FileExit" verb="" _label="_Quit" pixtype="stock" pixname="gtk-quit"/>

</submenu>

<submenu name="Edit" _label="_Edit">

        <menuitem name="Cut" verb="Cut" _label="Cu_t" accel="*Control*X"/>
	
        <menuitem name="Copy" verb="Copy" _label="_Copy" accel="*Control*c"/>
	
        <menuitem name="Paste" verb="Paste" _label="_Paste" accel="*Control*v"/>
	
	<separator/> 

	<placeholder name="EditMenu"/>

	<menuitem name="EditPrefs" 
         _label="P_references..." 
         _tip="Edit Nautilus preferences"
         pixtype="stock" pixname="gtk-preferences" 
         verb="EditPrefs"/>

</submenu>

<submenu name="Controls" _label="_Controls">

	<menuitem name="Play" _label="_Play" verb="Play"/>

	<separator/>
	
	<menuitem name="Next" _label="_Next Song" verb="Next"/>
	<menuitem name="Previous" _label="P_revious Song" verb="Previous"/>

	<separator/>
	
	<menuitem name="Shuffle" _label="_Shuffle" verb="Shuffle"/>
	<menuitem name="Repeat" _label="_Repeat" verb="Repeat"/>

</submenu>

<submenu name="Visuals" _label="_Visuals">

	<menuitem name="TurnOn" _label="Turn Visual _On" verb="" pixtype="stock" pixname="gtk-execute"/>
	
	<separator/>

	<menuitem name="TurnOn" _label="_Small" verb=""/>
	<menuitem name="TurnOn" _label="_Medium" verb=""/>
	<menuitem name="TurnOn" _label="_Large" verb=""/>

	<separator/>

	<menuitem name="Fullscreen" _label="_Fullscreen" verb=""/>

</submenu>

<placeholder name="ActionsMenu"/>

<submenu name="Help" _label="_Help">

	<menuitem name="HelpContents" verb="" _label="_Contents"
	pixtype="stock" pixname="gtk-help"/>
	
        <menuitem name="About" verb="" _label="_About..." pixtype="stock" pixname="gnome-stock-about"/>

</submenu>

</menu>

<dockitem name="Toolbar" behavior="exclusive" config="0">
	<toolitem name="Prevous" _label="Previous" verb="Previous"/>
	<toolitem name="Play" _label="Play" verb="Play"/>
	<toolitem name="Next" _label="Next" verb="Next"/>
	<separator/>
        <toolitem name="Shuffle" _label="Shuffle" verb="Shuffle"/> 
	<separator/>
        <toolitem name="Visuals" 
		  _label="Visuals"
		  priority="1"
		  _tip="Show visualization effects"
                  pixtype="stock" pixname="gtk-execute"
                  verb="Visuals"/> 
</dockitem>

</Root>
