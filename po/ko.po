#.
#: ../lib/rb-cut-and-paste-code.c:309
msgid "%b %d %I:%M %p"
msgstr "%b %d일 %I:%M %p"

#. Translators: "friendly time" string for a day in a different year,
#. * strftime format. e.g. "Feb 12 1997"
#.
#: ../lib/rb-cut-and-paste-code.c:314
msgid "%b %d %Y"
msgstr "%Y년 %b %d일"

#. impossible time or broken locale settings
