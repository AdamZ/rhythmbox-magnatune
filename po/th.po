#.
#: ../lib/rb-cut-and-paste-code.c:311
msgid "%b %d %I:%M %p"
msgstr "%d %b %H:%M"

#. Translators: "friendly time" string for a day in a different year,
#. * strftime format. e.g. "Feb 12 1997"
#.
#: ../lib/rb-cut-and-paste-code.c:316
msgid "%b %d %Y"
msgstr "%d %b %Ey"

